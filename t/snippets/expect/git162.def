if ( $x in : eq @some_strings ) {
    say "x is one of the given strings";
}

match( $n : == ) {
    case (1) { say "It's one" }
    case (2) { say "It's two" }
    case (3) { say "It's three" }
    case (4), case (5) { say "It's four or five" }
    case if ( $n < 10 ) { say "It's less than ten" }
    default             { say "It's something else" }
}
